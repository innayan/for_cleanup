      <runner id="RUNNER_158" name="" type="simpleRunner">
        <parameters>
          <param name="script.content" value="ping 127.0.0.1 -n 20 &gt; nul" />
          <param name="teamcity.step.mode" value="execute_if_failed" />
          <param name="use.custom.script" value="true" />
        </parameters>
