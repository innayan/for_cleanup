        <parameters>
          <param name="script.content"><![CDATA[echo test
echo "##teamcity[jacocoReport dataPath='jacoco.exec' includes='com.package.core.*' classpath='classes/lib/some.jar' reportDir='temp/jacocoReport' toolVersion='0.7.8']"
