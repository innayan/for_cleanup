      <policy type="days" cleanup-level="ARTIFACTS">
        <parameters>
          <param name="artifactPatterns" value="**/*.*" />
          <param name="keepDays.count" value="1" />
        </parameters>
      </policy>
    </cleanup>
  </settings>
</build-type>

